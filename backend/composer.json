{
    "name": "martinlejko/owleyes",
    "description": "A simple monitoring service similar to UptimeRobot",
    "type": "project",
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "slim/slim": "^4.11",
        "slim/psr7": "^1.6",
        "doctrine/orm": "^2.14",
        "doctrine/migrations": "^3.5",
        "monolog/monolog": "^2.9",
        "php-di/php-di": "^6.4",
        "php-di/slim-bridge": "^3.2",
        "symfony/cache": "^5.4",
        "webonyx/graphql-php": "^14.11",
        "vlucas/phpdotenv": "^5.5",
        "guzzlehttp/guzzle": "^7.5",
        "tuupola/slim-jwt-auth": "^3.7"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "squizlabs/php_codesniffer": "^3.7"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "phpcs": "phpcs",
        "phpcbf": "phpcbf",
        "test": "phpunit",
        "start": "php -S localhost:8080 -t public"
    }
} 